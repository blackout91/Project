<% if @results.length == 0 %>
        <p>No search results found!</p>
    <% else %>
        <br />
         <table>
            <tr>
                <th bgcolour="#3A2F0B">Item</th>
                <th bgcolour="#0B1907">Small</th>
                <th bgcolour="#071418">Medium</th>
                <th bgcolour="#120A2A">Large</th>
            </tr>
            <% @arr.each do |record| %>
                <tr>
                    <td><%=h record %></td>
                    <td>£<%=h @db.get_first_value("SELECT price FROM menu WHERE size = 'Small' AND item = ?", [record]) %></td>
                    <td>£<%=h @db.get_first_value("SELECT price FROM menu WHERE size = 'Medium' AND item = ?", [record])%></td>
                    <td>£<%=h @db.get_first_value("SELECT price FROM menu WHERE size = 'Large' AND item = ?", [record])%></td>
                </tr>
            <% end %>
        </table>

        <br>
        <img src="https://www.simplyrecipes.com/wp-content/uploads/2007/05/rhubarb-berry-pie-horiz-a-1600.jpg" alt="" style="width:19%">
        <img src="https://cdn-eu-ec.yottaa.net/55b635db0b5344273c002031/d1fd69005c1501336a81123dfe2baf36.yottaa.net/v~22.3a7/6655-3-large.jpg?yocs=H_&yoloc=eu" alt="" style="width:19%">
        <img src="data:image/jpeg;base64," alt="" style="width:19%">
        <img src="https://www.inspiredtaste.net/wp-content/uploads/2016/06/Blueberry-Pie-Recipe-2-1200.jpg" alt="" style="width:19%">
        <img src="https://www.simplyrecipes.com/wp-content/uploads/2017/06/american-flag-pie-horiz-a2-1200.jpg" alt="" style="width:19%">

    <% end %>